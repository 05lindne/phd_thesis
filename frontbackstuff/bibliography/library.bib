issn = {00027863},
journal = {J. Am. Chem. Soc.},
number = {35},
pages = {11635--11642},
pmid = {16939289},
title = {{Control of sp2/sp3 carbon ratio and surface chemistry of nanodiamond powders by selective oxidation in air}},
volume = {128},
year = {2006}
}
