issn = {1367-2630},
journal = {New Journal of Physics},
month = {may},
number = {5},
pages = {053005},
title = {{Native NIR-emitting single colour centres in CVD diamond}},
url = {http://stacks.iop.org/1367-2630/16/i=5/a=053005},
volume = {16},
year = {2014}
