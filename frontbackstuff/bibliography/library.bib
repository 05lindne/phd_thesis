journal = {Diamond and Related Materials},
keywords = {737 nm,Gem diamond,Natural diamond,Si-V defect},
number = {7-10},
pages = {1335--1344},
title = {{Occurrence of the Si-V defect center in natural colorless gem diamonds}},
volume = {17},
