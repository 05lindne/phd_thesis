@article{Benedikter2017a,
abstract = {Single photon sources are an integral part of various quantum technologies, and solid state quantum emitters at room temperature appear as a promising implementation. We couple the fluorescence of individual silicon vacancy centers in nanodiamonds to a tunable optical microcavity to demonstrate a single photon source with high efficiency, increased emission rate, and improved spectral purity compared to the intrinsic emitter properties. We use a fiber-based microcavity with a mode volume as small as {\$}3.4{\~{}}\backslashlambda{\^{}}3{\$} and a quality factor of {\$}1.9\backslashtimes 10{\^{}}4{\$} and observe an effective Purcell factor of up to 9.2. We furthermore study modifications of the internal rate dynamics and propose a rate model that closely agrees with the measurements. We observe lifetime changes of up to 31{\%}, limited by the finite quantum efficiency of the emitters studied here. With improved materials, our achieved parameters predict single photon rates beyond 1 GHz.},
archivePrefix = {arXiv},
arxivId = {1612.05509},
author = {Benedikter, Julia and Kaupp, Hanno and H{\"{u}}mmer, Thomas and Liang, Yuejiang and Bommer, Alexander and Becher, Christoph and Krueger, Anke and Smith, Jason M. and H{\"{a}}nsch, Theodor W. and Hunger, David},
doi = {10.1103/PhysRevApplied.7.024031},
eprint = {1612.05509},
file = {:Users/sahara/Documents/Mendeley Desktop/Benedikter et al. - 2017 - Cavity-Enhanced Single-Photon Source Based on the Silicon-Vacancy Center in Diamond(2).pdf:pdf},
issn = {23317019},
journal = {Physical Review Applied},
number = {2},
pages = {024031},
title = {{Cavity-Enhanced Single-Photon Source Based on the Silicon-Vacancy Center in Diamond}},
volume = {7},
year = {2017}
}
@article{Iii1991,
abstract = {The frequency and line shape of the diamond Raman line are examined in detail for a series of microwave-plasma-assisted chemical-vapor-deposition films grown on Si. The Raman lines in the films appear at higher frequency (shifts of up to 3 cm-1) than that of natural diamond and the observed lines are symmetric with broader linewidths than that of natural diamond, ranging from 5.7 to 17.1 cm-1. In addition, the line frequencies and linewidths are correlated; the films with the highest vibrational frequencies have the largest linewidths. The data include single-point measurements on eight films grown under different conditions as well as 500 data points from different positions on a single film that were obtained in a spatially resolved Raman experiment. Several mechanisms for the frequency shift and the correlation of the linewidth with frequency are considered including phonon confinement, residual stress, and defect scattering. Contrary to the observations, Raman line shapes computed from the phonon-confinement model (which has been used successfully to model Raman scattering in microcrystalline Si and GaAs), using phonon-dispersion curves for diamond from the literature, are highly asymmetric at the linewidths observed. It is concluded that the observed shifts in the diamond Raman line do not arise from phonon confinement alone and arise primarily from compressive stress. The line broadening also is not produced by phonon confinement alone and may arise from decreasing phonon lifetime associated with scattering from defects or from an inhomogeneous stress distribution in the films. The observed correlation between Raman line frequency and width suggests that the degree of compressive stress may be associated with the density of microcrystalline defects.},
author = {Iii, Jw Ager and Veirs, Dk and Rosenblatt, Gm},
doi = {10.1103/PhysRevB.43.6491},
file = {:Users/sahara/Documents/Mendeley Desktop/Iii, Veirs, Rosenblatt - 1991 - Spatially resolved Raman studies of diamond films grown by chemical vapor deposition.pdf:pdf},
issn = {0163-1829},
journal = {Physical Review B},
keywords = {Infrared and Raman spectra,Phonons in crystal lattices},
number = {8},
pages = {6491--6500},
pmid = {9998089},
title = {{Spatially resolved Raman studies of diamond films grown by chemical vapor deposition}},
url = {http://prb.aps.org/abstract/PRB/v43/i8/p6491{\%}7B{\_}{\%}7D1 http://prb.aps.org/abstract/PRB/v43/i8/p6491{\_}1},
volume = {43},
year = {1991}
}
