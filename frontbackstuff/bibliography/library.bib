issn = {2334-2536},
journal = {Optica},
number = {1},
pages = {71},
title = {{Experimental realization of an absolute single-photon source based on a single nitrogen vacancy center in a nanodiamond}},
url = {https://www.osapublishing.org/abstract.cfm?URI=optica-4-1-71},
volume = {4},
